{
	"name": "@savantly/ngxSecurity",
	"version": "1.0.0",
	"description": "Security service for Angular app",
	"main": "dist/index.js",
	"module": "dist/ngxSecurity.umd.js",
	"homepage": "https://github.com/savantly-net/ngxSecurity",
	"repository": {
		"type": "git",
		"url": "https://github.com/savantly-net/ngxSecurity.git"
	},
	"scripts": {
		"build": "rimraf dist && npm run transpile && npm run package && npm run minify",
		"ci": "npm run e2e && npm run test",
		"clean": "rimraf node_modules doc dist && npm cache clean",
		"clean-install": "npm run clean && npm install",
		"clean-start": "npm run clean-install && npm start",
		"docs": "typedoc --options typedoc.json src/app/app.component.ts",
		"e2e": "protractor",
		"e2e-live": "protractor --elementExplorer",
		"lint": "tslint --project tsconfig.json --force \"src/**/*.ts\"",
		"minify": "uglifyjs dist/ngxSecurity.umd.js --screw-ie8 --compress --mangle --comments --output dist/ngxSecurity.umd.min.js",
		"package": "rollup -c",
		"postinstall": "npm run update-webdriver && typings install",
		"posttest": "remap-istanbul -i coverage/json/coverage-final.json -o coverage/html -t html",
		"pretest": "npm run lint",
		"server": "webpack-dev-server --inline --progress --port 8080",
		"start": "npm run server",
		"start:hmr": "npm run server -- --hot",
		"test": "karma start",
		"test-watch": "karma start --no-single-run --auto-watch",
		"transpile": "ngc -p tsconfig.build.json",
		"update-webdriver": "webdriver-manager update",
		"watch": "webpack --watch --progress --profile",
		"webdriver-update": "webdriver-manager update",
		"webdriver-start": "webdriver-manager start"
	},
	"keywords": [
		"Sprout",
		"Security",
		"Angular"
	],
	"author": "Jeremy@Savantly.net",
	"license": "MIT",
	"dependencies": {
		"@angular/common": "^4.4.3",
		"@angular/core": "^4.4.3",
		"@angular/router": "^4.4.3",
		"jwt-decode": "^2.2.0",
		"rxjs": "^5.4.3",
		"zone.js": "^0.8.17"
	},
	"devDependencies": {
		"@angular/cli": "^1.4.2",
		"@angular/compiler": "^4.4.3",
		"@angular/compiler-cli": "^4.4.3",
		"@angular/platform-browser": "^4.4.3",
		"@angular/platform-browser-dynamic": "^4.4.3",
		"@angularclass/hmr": "^2.1.3",
		"@angularclass/hmr-loader": "^3.0.4",
		"@types/jasmine": "^2.6.0",
		"@types/jwt-decode": "^2.2.1",
		"@types/node": "^8.0.28",
		"@types/selenium-webdriver": "^3.0.7",
		"angular2-template-loader": "^0.6.2",
		"autoprefixer": "^7.1.4",
		"awesome-typescript-loader": "^3.2.3",
		"codelyzer": "^3.2.0",
		"copy-webpack-plugin": "^4.0.1",
		"core-js": "^2.5.1",
		"css-loader": "^0.28.7",
		"extract-text-webpack-plugin": "^3.0.0",
		"file-loader": "^0.11.2",
		"html-loader": "^0.5.1",
		"html-webpack-plugin": "^2.30.1",
		"istanbul-instrumenter-loader": "^3.0.0",
		"jasmine-core": "^2.8.0",
		"jasmine-spec-reporter": "^4.2.1",
		"json-loader": "^0.5.7",
		"karma": "^1.7.1",
		"karma-chrome-launcher": "^2.2.0",
		"karma-coverage": "^1.1.1",
		"karma-jasmine": "^1.1.0",
		"karma-mocha-reporter": "^2.2.4",
		"karma-phantomjs-launcher": "^1.0.4",
		"karma-sourcemap-loader": "^0.3.7",
		"karma-webpack": "^2.0.4",
		"node-sass": "^4.5.3",
		"null-loader": "^0.1.1",
		"postcss-loader": "^1.1.0",
		"protractor": "^5.1.2",
		"raw-loader": "^0.5.1",
		"remap-istanbul": "^0.9.5",
		"rimraf": "^2.6.2",
		"rollup": "^0.50.0",
		"rollup-plugin-commonjs": "^8.2.1",
		"rollup-plugin-copy": "^0.2.3",
		"rollup-plugin-node-resolve": "^3.0.0",
		"sass-loader": "^6.0.6",
		"shelljs": "^0.7.8",
		"style-loader": "^0.18.2",
		"ts-helpers": "^1.1.2",
		"ts-loader": "^2.3.7",
		"tslint": "^5.7.0",
		"tslint-loader": "^3.5.3",
		"typedoc": "^0.8.0",
		"typescript": "^2.5.2",
		"uglify-js": "^3.1.1",
		"url-loader": "^0.5.9",
		"webpack": "^3.6.0",
		"webpack-dev-server": "^2.8.2"
	}
}
